    "class_type": "VAEDecode",
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "10": {
    "inputs": {
      "destination": ["1", 0],
      "source": ["9", 0],
      "mask": ["4", 0]
    },
    "class_type": "ImageCompositeMasked",
    "_meta": {
      "title": "Composite: Paste Inpainted Region Back"
    }
  },
  "11": {
    "inputs": {
      "filename_prefix": "InpaintedPhoto",
      "images": ["10", 0]
