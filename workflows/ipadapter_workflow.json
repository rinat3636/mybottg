{
  "1": {
    "inputs": {
      "image": "input_image.png",
      "upload": "image"
    },
    "class_type": "LoadImage",
    "_meta": {
      "title": "Load Input Image"
    }
  },
  "2": {
    "inputs": {
      "ckpt_name": "sd_xl_base_1.0.safetensors"
    },
    "class_type": "CheckpointLoaderSimple",
    "_meta": {
      "title": "Load SDXL Checkpoint"
    }
  },
  "3": {
    "inputs": {
      "ipadapter_file": "ip-adapter_sdxl.safetensors"
    },
    "class_type": "IPAdapterModelLoader",
    "_meta": {
      "title": "Load IPAdapter SDXL"
    }
  },
  "4": {
    "inputs": {
      "clip_name": "CLIP-ViT-bigG-14-laion2B-39k-b79k.safetensors"
    },
    "class_type": "CLIPVisionLoader",
    "_meta": {
      "title": "Load CLIP Vision"
    }
  },
  "5": {
    "inputs": {
      "model": ["2", 0],
      "ipadapter": ["3", 0],
      "image": ["1", 0],
      "weight": 0.7,
      "weight_type": "linear",
      "combine_embeds": "concat",
      "start_at": 0.0,
      "end_at": 0.9,
      "embeds_scaling": "V only",
      "clip_vision": ["4", 0]
    },
    "class_type": "IPAdapterAdvanced",
    "_meta": {
      "title": "Apply IPAdapter (preserve appearance)"
    }
  },
  "6": {
    "inputs": {
      "image": ["1", 0],
      "low_threshold": 0.3,
      "high_threshold": 0.7
    },
    "class_type": "Canny",
    "_meta": {
      "title": "Canny Edge Detection"
    }
  },
  "7": {
    "inputs": {
      "control_net_name": "controlnet-canny-sdxl.safetensors"
    },
    "class_type": "ControlNetLoader",
    "_meta": {
      "title": "Load ControlNet Canny SDXL"
    }
  },
  "8": {
    "inputs": {
      "text": "a person, high quality, detailed, photorealistic",
      "clip": ["2", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "9": {
    "inputs": {
      "text": "text, watermark, low quality, blurry, deformed, ugly, bad anatomy, extra limbs, disfigured",
      "clip": ["2", 1]
    },
    "class_type": "CLIPTextEncode",
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "10": {
    "inputs": {
      "positive": ["8", 0],
      "negative": ["9", 0],
      "control_net": ["7", 0],
      "image": ["6", 0],
      "strength": 0.5,
      "start_percent": 0.0,
      "end_percent": 0.7
    },
    "class_type": "ControlNetApplyAdvanced",
    "_meta": {
      "title": "Apply ControlNet Canny (light - preserve pose/structure)"
    }
  },
  "11": {
    "inputs": {
      "pixels": ["1", 0],
      "vae": ["2", 2]
    },
    "class_type": "VAEEncode",
    "_meta": {
      "title": "VAE Encode (img2img)"
    }
  },
  "12": {
    "inputs": {
      "seed": 42,
      "steps": 30,
      "cfg": 6.5,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 0.80,
      "model": ["5", 0],
      "positive": ["10", 0],
      "negative": ["10", 1],
      "latent_image": ["11", 0]
    },
    "class_type": "KSampler",
    "_meta": {
      "title": "KSampler (IPAdapter + ControlNet img2img)"
    }
  },
  "13": {
    "inputs": {
      "samples": ["12", 0],
      "vae": ["2", 2]
    },
    "class_type": "VAEDecode",
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "14": {
    "inputs": {
      "filename_prefix": "EditedPhoto",
      "images": ["13", 0]
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Image"
    }
  }
}
